# Anthropic Claude API Key
# Get your API key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenWeatherMap API Key
# Get your free API key from: https://openweathermap.org/api
OPENWEATHERMAP_API_KEY=your_openweathermap_api_key_here

# LangSmith (Optional - for LangChain monitoring and debugging)
# Get your API key from: https://smith.langchain.com/
# Set to "true" to enable tracing
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=ai-chat-weather

# Langfuse (Optional - for LLM application observability)
# Get your keys from: https://cloud.langfuse.com/
# Set to "true" to enable tracing
LANGFUSE_ENABLED=false
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key_here
LANGFUSE_SECRET_KEY=your_langfuse_secret_key_here
LANGFUSE_HOST=https://cloud.langfuse.com
